package aula1_testeGerenteDiretor;

public class TestaTipo {
	public static void main(String[] args) {
		Funcionario comum = new Funcionario();
		comum.setTipo(0);
		comum.setSalario(3000);
		System.out.println("Bonificacao de funcionario comum: " + comum.getBonificacao());

		Funcionario gerente = new Funcionario();
		gerente.setTipo(1);
		gerente.setSalario(4000);
		System.out.println("Bonificacao de gerente: " + gerente.getBonificacao());

		Funcionario diretor = new Funcionario();
		diretor.setTipo(2);
		diretor.setSalario(6000);
		System.out.println("Bonificacao de diretor: " + diretor.getBonificacao());
	}
}
