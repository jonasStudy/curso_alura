package bytebank_herdado;

public class Funcionario {
	private String nome;
	private String cfp;
	private double salario;

	public double getBonificacao() {
		return this.salario * 0.1;
	}

	public String getNome() {
		return this.nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}
}