package bytebank_herdado;

public class Gerente extends Funcionario {
	private int senha;

	public boolean autenticar(int senha) {
		if (this.senha == senha) {
			return true;
		}
		return false;
	}
	
	public void setSenha(int senha) {
		this.senha = senha;
	}

//	public double getBonificacao() {
//		return this.salario;
//	}
}